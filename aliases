# =======================================================================
# ==Alias
# =======================================================================
alias r="ranger"
alias ls="exa"
alias wgetall="wget -m -k -p -nc -E"
alias ftcount="fd -t f . | rg '.*\\.[a-zA-Z0-9]*$' | sed -e 's/.*\\(\\.[a-zA-Z0-9]*\\)$/\\1/' | sort | uniq -c | sort -n"
